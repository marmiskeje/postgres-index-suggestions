  <data name="SummaryReport" xml:space="preserve">
    <value>&lt;!doctype html&gt;
&lt;html&gt;
&lt;head&gt;
    &lt;meta charset="UTF-8"&gt;
    &lt;title&gt;DBMS statistics - summary report&lt;/title&gt;
    &lt;style&gt;
        body {
            margin: 10px;
            font-family: 'Open Sans', arial;
            font-size: 16px;
        }

        table {
            border-collapse: collapse;
            border: 1px solid #38678f;
        }

        th {
            background: #4682b4;
            border: 1px solid #38678f;
            text-shadow: 0 1px 0 #38678f;
            box-shadow: inset 0px 1px 2px #568ebd;
            color: white;
            height: 20px;
            padding: 10px;
        }

        td {
            border-left: 1px solid #cccccc;
            border-right: 1px solid #cccccc;
            border-bottom: 1px solid #cccccc;
            padding: 10px;
        }

        .featured {
			background: #edf3f8;
        }
		
		.center {
			text-align: center;
		}
		
		h2 {
			text-transform: uppercase;
		}
		
		h2:after,
		h2:before{
			content:"\00a0\00a0\00a0\00a0\00a0";
			text-decoration:line-through;
		}
		
		.leftIndent {
			padding-left: 20px;
		}
    &lt;/style&gt;
&lt;/head&gt;
&lt;body&gt;
	&lt;h1 class="center"&gt;DBMS statistics - summary report&lt;h1&gt;
	&lt;h3 class="center"&gt;(@(Model.ToDateString(Model.PeriodFrom)) - @(Model.ToDateString(Model.PeriodTo)))&lt;/h3&gt;
	&lt;div class="leftIndent"&gt;
		@foreach (var db in Model.Databases)
        {
			&lt;h2&gt;Database @(db.Name)&lt;/h2&gt;
			&lt;h3&gt;TOP 10 most frequently executed statements:&lt;/h3&gt;
			&lt;table&gt;
				&lt;tr&gt;&lt;th&gt;#&lt;/th&gt;&lt;th&gt;Statement&lt;/th&gt;&lt;th&gt;Execution count&lt;/th&gt;&lt;th&gt;Max duration&lt;/th&gt;&lt;th&gt;Avg duration&lt;/th&gt;&lt;th&gt;Min duration&lt;/th&gt;&lt;th&gt;Total duration&lt;/th&gt;&lt;tr&gt;
				@if (db.TopExecutedStatements.Count &gt; 0)
				{
					@for (int i = 0; i &lt; db.TopExecutedStatements.Count; i++)
					{
						&lt;tr&gt;
							&lt;td&gt;@(i + 1)&lt;/td&gt;
							&lt;td&gt;@db.TopExecutedStatements[i].Statement&lt;/td&gt;
							&lt;td class="featured"&gt;@db.TopExecutedStatements[i].ExecutionCount&lt;/td&gt;
							&lt;td&gt;@db.TopExecutedStatements[i].MaxDuration&lt;/td&gt;
							&lt;td&gt;@db.TopExecutedStatements[i].AvgDuration&lt;/td&gt;
							&lt;td&gt;@db.TopExecutedStatements[i].MinDuration&lt;/td&gt;
							&lt;td&gt;@db.TopExecutedStatements[i].TotalDuration&lt;/td&gt;
						&lt;/tr&gt;
					}
				}
				else
				{
					&lt;tr&gt;
						&lt;td colspan="7"&gt;No data&lt;/td&gt;
					&lt;/tr&gt;
				}
			&lt;/table&gt;
			&lt;br /&gt;
			&lt;h3&gt;TOP 10 slowest statements:&lt;/h3&gt;
			&lt;table&gt;
				&lt;tr&gt;&lt;th&gt;#&lt;/th&gt;&lt;th&gt;Statement&lt;/th&gt;&lt;th&gt;Execution count&lt;/th&gt;&lt;th&gt;Max duration&lt;/th&gt;&lt;th&gt;Avg duration&lt;/th&gt;&lt;th&gt;Min duration&lt;/th&gt;&lt;th&gt;Total duration&lt;/th&gt;&lt;tr&gt;
				@if (db.TopSlowestStatements.Count &gt; 0)
				{
					@for (int i = 0; i &lt; db.TopSlowestStatements.Count; i++)
					{
						&lt;tr&gt;
							&lt;td&gt;@(i + 1)&lt;/td&gt;
							&lt;td&gt;@db.TopSlowestStatements[i].Statement&lt;/td&gt;
							&lt;td&gt;@db.TopSlowestStatements[i].ExecutionCount&lt;/td&gt;
							&lt;td class="featured"&gt;@db.TopSlowestStatements[i].MaxDuration&lt;/td&gt;
							&lt;td&gt;@db.TopSlowestStatements[i].AvgDuration&lt;/td&gt;
							&lt;td&gt;@db.TopSlowestStatements[i].MinDuration&lt;/td&gt;
							&lt;td&gt;@db.TopSlowestStatements[i].TotalDuration&lt;/td&gt;
						&lt;/tr&gt;
					}
				}
				else
				{
					&lt;tr&gt;
						&lt;td colspan="7"&gt;No data&lt;/td&gt;
					&lt;/tr&gt;
				}
			&lt;/table&gt;
			&lt;br /&gt;
			&lt;h3&gt;TOP 10 alive relations:&lt;/h3&gt;
			&lt;table&gt;
				&lt;tr&gt;&lt;th&gt;#&lt;/th&gt;&lt;th&gt;Relation&lt;/th&gt;&lt;th&gt;# Seq scans&lt;/th&gt;&lt;th&gt;# Index scans&lt;/th&gt;&lt;th&gt;# Inserted tuples&lt;/th&gt;&lt;th&gt;# Updated tuples&lt;/th&gt;&lt;th&gt;# Deleted tuples&lt;/th&gt;&lt;th&gt;# Total&lt;/th&gt;&lt;tr&gt;
				@if (db.TopAliveRelations.Count &gt; 0)
				{
					@for (int i = 0; i &lt; db.TopAliveRelations.Count; i++)
					{
						&lt;tr&gt;
							&lt;td&gt;@(i + 1)&lt;/td&gt;
							&lt;td&gt;@db.TopAliveRelations[i].Name&lt;/td&gt;
							&lt;td &gt;@db.TopAliveRelations[i].SeqScansCount&lt;/td&gt;
							&lt;td&gt;@db.TopAliveRelations[i].IndexScansCount&lt;/td&gt;
							&lt;td&gt;@db.TopAliveRelations[i].TuplesInsertCount&lt;/td&gt;
							&lt;td&gt;@db.TopAliveRelations[i].TuplesUpdateCount&lt;/td&gt;
							&lt;td&gt;@db.TopAliveRelations[i].TuplesDeleteCount&lt;/td&gt;
							&lt;td class="featured"&gt;@db.TopAliveRelations[i].TotalCount&lt;/td&gt;
						&lt;/tr&gt;
					}
				}
				else
				{
					&lt;tr&gt;
						&lt;td colspan="8"&gt;No data&lt;/td&gt;
					&lt;/tr&gt;
				}
			&lt;/table&gt;
		}
	&lt;/div&gt;
&lt;/body&gt;
&lt;/html&gt;</value>
  </data>
